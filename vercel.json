{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        { "src": "/([a-z]{2,3})/about", "dest": "/public/about.html" },
        { "src": "/([a-z]{2,3})/services", "dest": "/public/services.html" },
        { "src": "/([a-z]{2,3})/contact", "dest": "/public/contact.html" },
        { "src": "/([a-z]{2,3})/privacy", "dest": "/public/privacy.html" },
        { "src": "/([a-z]{2,3})/terms", "dest": "/public/terms.html" },
        { "src": "/([a-z]{2,3})/aml-policy", "dest": "/public/aml-policy.html" },
        { "src": "/([a-z]{2,3})/sitemap.xml", "dest": "/public/sitemap.xml" },
        { "src": "/([a-z]{2,3})", "dest": "/public/index.html" },
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/privacy",
            "dest": "/public/privacy.html"
        },
        {
            "src": "/sitemap.xml",
            "dest": "/public/sitemap.xml"
        },
        {
            "src": "/terms",
            "dest": "/public/terms.html"
        },
        {
            "src": "/aml-policy",
            "dest": "/public/aml-policy.html"
        },
        {
            "src": "/about",
            "dest": "/public/about.html"
        },
        {
            "src": "/services",
            "dest": "/public/services.html"
        },
        {
            "src": "/contact",
            "dest": "/public/contact.html"
        },
        {
            "src": "/",
            "dest": "/public/index.html"
        },
        {
            "src": "/css/(.*)",
            "dest": "/public/css/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "/public/js/$1"
        },
        {
            "src": "/fonts/(.*)",
            "dest": "/public/fonts/$1"
        },
        {
            "src": "/img/(.*)",
            "dest": "/public/img/$1"
        },
        {
        "src": "/([a-z]{2,3})/css/(.*)",
        "dest": "/public/css/$2"
        },
        {
        "src": "/([a-z]{2,3})/js/(.*)",
        "dest": "/public/js/$2"
        },
        {
        "src": "/([a-z]{2,3})/fonts/(.*)",
        "dest": "/public/fonts/$2"
        },
        {
        "src": "/([a-z]{2,3})/img/(.*)",
        "dest": "/public/img/$2"
        }
    ]
}